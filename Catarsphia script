-- map loots find
for _,v in pairs(game:GetService("Workspace").Loot:GetChildren()) do
	if v:FindFirstChild("Handle") ~= nil then	
		addUI(v.Handle)
	end
end
game:GetService("Workspace").Loot.ChildAdded:Connect(function(v)
	addUI(v:WaitForChild("Handle"))
end)

-- map Resource?
for _,v in pairs(game:GetService("Workspace").Suroviny:GetChildren()) do
	if v.Name == "PineTree" or v.Name == "OakTree" or v.Name == "Bush" or v.Name == "Grass" or v.Name == "Barrel" then
		Resource(v)
	else
		Ore(v)
	end
end
game:GetService("Workspace").Suroviny.ChildAdded:Connect(function(v)
	if v.Name == "PineTree" or v.Name == "OakTree" or v.Name == "Bush" or v.Name == "Grass" or v.Name == "Barrel" then
		Resource(v)
	else
		Ore(v)
	end
end)

--Animal
for _,v in pairs(game:GetService("Workspace").Animals:GetChildren()) do
	if v:FindFirstChild("Head") ~= nil then	
		Animal(v.Head)
	end
end
game:GetService("Workspace").Animals.ChildAdded:Connect(function(v)
	Animal(v:WaitForChild("Head"))
end)

