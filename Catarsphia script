--SpeedVariables
local plr = game:GetService("Players").LocalPlayer
local char = plr.Character
local mouse = game:GetService("Players").LocalPlayer:GetMouse()
local hum = char:FindFirstChild("HumanoidRootPart")
--yeet
Speed.MouseButton1Down:connect(function()
	local plr = game:GetService("Players").LocalPlayer
	local char = plr.Character
	local mouse = game:GetService("Players").LocalPlayer:GetMouse()
	local hum = char:FindFirstChild("HumanoidRootPart")
	mouse.KeyDown:connect(function(key)
		if key == SpeedBind.Text then
			loop = true
			while loop do
				hum.CFrame = hum.CFrame + hum.CFrame.lookVector * 6.5
				wait(0.35)
			end
		end
	end)
end)

mouse.KeyUp:connect(function(key)
	if key == SpeedBind.Text then
		loop = false
	end
end)


function addUI(part)
	if itemEsp == true then

		local pn = part.Parent.Name

		local partGui = Instance.new("BillboardGui", part)
		partGui.Size = UDim2.new(1,0,1,0)
		partGui.AlwaysOnTop = true
		partGui.Name = "Iten-ESP"
		local frame = Instance.new("Frame", partGui)
		frame.BackgroundColor3 = Color3.fromRGB(255,80,60)
		frame.BackgroundTransparency = 0.75
		frame.Size = UDim2.new(1,0,1,0)
		frame.BorderSizePixel = 0
		local nameGui = Instance.new("BillboardGui", part)
		nameGui.Size = UDim2.new(3,0,1.5,0)
		nameGui.SizeOffset = Vector2.new(0,1)
		nameGui.AlwaysOnTop = true
		nameGui.Name = "Name"
		local text = Instance.new("TextLabel", nameGui)
		text.Text = pn
		text.TextColor3 = Color3.fromRGB(255,80,60)
		text.TextTransparency = 0.25
		text.BackgroundTransparency = 1
		text.TextSize = 18
		text.Size = UDim2.new(1,0,1,0)
		text.Font = Enum.Font.GothamSemibold
		text.Name = "Text"
		if pn == "Iron" then
			text.TextColor3 = Color3.fromRGB(40, 80, 240)
			text.TextSize = 32
			print("Iron! ".. pn)
		elseif pn == "Sulfur" then
			text.TextColor3 = Color3.fromRGB(240, 240, 80)
			text.TextSize = 36
			print("Sulfur! ".. pn)
		elseif pn == "SmgAmmo" or pn == "ShotgunAmmo" or pn == "PistolAmmo" then
			text.TextColor3 = Color3.fromRGB(150, 0, 0)
			text.TextSize = 38
			print("Ammo!".. pn)
		elseif pn == "Wood" then
			text.TextColor3 = Color3.fromRGB(160, 105, 55)
			text.TextSize = 24
			print("Wood! ".. pn)
		elseif pn == "Stone" then
			text.TextColor3 = Color3.fromRGB(150, 150, 150)
			text.TextSize = 24
			print("Stone! ".. pn)
		elseif pn == "IronOre" or pn == "SulfurOre" then
			text.TextColor3 = Color3.fromRGB(250, 250, 250)
			text.TextSize = 28
			print("Ore! ".. pn)
		elseif pn == "ChestPrivate" then
			text.TextColor3 = Color3.fromRGB(155, 5, 5)
			text.TextSize = 36
			print("Private Locker!!! ".. pn)
		elseif pn == "Foundation" or pn == "StoneWall" or pn == "StoneWindow" or pn == "StoneDoorway" then
			text.TextColor3 = Color3.fromRGB(80, 80, 80)
			text.TextSize = 24
			print("Stone Building! ".. pn)
		elseif pn == "MilitaryPants" or pn == "MilitaryShirt" then
			text.TextColor3 = Color3.fromRGB(47, 220, 62)
			text.TextSize = 26
			print("Military Armor! ".. pn)
		elseif pn == "XmasLights" then
			text.TextColor3 = Color3.fromRGB(29, 189, 1)
			text.TextSize = 22
			print("XmasLight ".. pn)
		elseif pn == "XmasPants" or pn == "XmasShirt" then
			text.TextColor3 = Color3.fromRGB(46, 255, 0)
			text.TextSize = 30
			print("Xmas Armor! ".. pn)
		else
			print("no... ".. pn)
		end
	end
end


function Ore(part)
	if OresEsp == true then

		local pn = part.Name

		local partGui = Instance.new("BillboardGui", part)
		partGui.Size = UDim2.new(1,0,1,0)
		partGui.AlwaysOnTop = true
		partGui.Name = "Iten-ESP"
		local frame = Instance.new("Frame", partGui)
		frame.BackgroundColor3 = Color3.fromRGB(63, 67, 71)
		frame.BackgroundTransparency = 0.5
		frame.Size = UDim2.new(1,0,1,0)
		frame.BorderSizePixel = 0
		local nameGui = Instance.new("BillboardGui", part)
		nameGui.Size = UDim2.new(3,0,1.5,0)
		nameGui.SizeOffset = Vector2.new(0,1)
		nameGui.AlwaysOnTop = true
		nameGui.Name = "Name"
		local text = Instance.new("TextLabel", nameGui)
		text.Text = pn
		text.TextColor3 = Color3.fromRGB(63, 67, 71)
		text.TextTransparency = 0.45
		text.BackgroundTransparency = 1
		text.TextSize = 16
		text.Size = UDim2.new(1,0,1,0)
		text.Font = Enum.Font.GothamSemibold
		text.Name = "Text"

		if pn == "IronOre" then
			text.TextColor3 = Color3.fromRGB(170, 255, 255)
			text.TextSize = 20
			text.TextTransparency = 0.40
		elseif pn == "SulfurOre" then
			text.TextColor3 = Color3.fromRGB(255, 255, 0)
			text.TextSize = 24
			text.TextTransparency = 0.35
		end
	end
end

function Resource(part)
	if ResourceEsp == true then

		local pn = part.Name

		local partGui = Instance.new("BillboardGui", part)
		partGui.Size = UDim2.new(1,0,1,0)
		partGui.AlwaysOnTop = true
		partGui.Name = "Iten-ESP"
		local frame = Instance.new("Frame", partGui)
		frame.BackgroundColor3 = Color3.fromRGB(135, 88, 41)
		frame.BackgroundTransparency = 0.75
		frame.Size = UDim2.new(1,0,1,0)
		frame.BorderSizePixel = 0
		local nameGui = Instance.new("BillboardGui", part)
		nameGui.Size = UDim2.new(3,0,1.5,0)
		nameGui.SizeOffset = Vector2.new(0,1)
		nameGui.AlwaysOnTop = true
		nameGui.Name = "Name"
		local text = Instance.new("TextLabel", nameGui)
		text.Text = pn
		text.TextColor3 = Color3.fromRGB(19, 120, 17)
		text.TextTransparency = 0.85
		text.BackgroundTransparency = 1
		text.TextSize = 6
		text.Size = UDim2.new(1,0,1,0)
		text.Font = Enum.Font.GothamSemibold
		text.Name = "Text"

		if pn == "Grass" or pn == "Bush" then
			text.TextColor3 = Color3.fromRGB(54, 163, 0)
			text.TextSize = 14
			text.TextTransparency = 0.35
		elseif pn == "Barrel" then
			text.TextColor3 = Color3.fromRGB(58, 0, 0)
			text.TextSize = 12
			text.TextTransparency = 0.45	
		end
	end

end

function Animal(part)

	local pn = part.Parent.Name

	local partGui = Instance.new("BillboardGui", part)
	partGui.Size = UDim2.new(1,0,1,0)
	partGui.AlwaysOnTop = true
	partGui.Name = "Iten-ESP"
	local frame = Instance.new("Frame", partGui)
	frame.BackgroundColor3 = Color3.fromRGB(170, 85, 0)
	frame.BackgroundTransparency = 0.5
	frame.Size = UDim2.new(1,0,1,0)
	frame.BorderSizePixel = 0
	local nameGui = Instance.new("BillboardGui", part)
	nameGui.Size = UDim2.new(3,0,1.5,0)
	nameGui.SizeOffset = Vector2.new(0,1)
	nameGui.AlwaysOnTop = true
	nameGui.Name = "Name"
	local text = Instance.new("TextLabel", nameGui)
	text.Text = pn
	text.TextColor3 = Color3.fromRGB(170, 85, 0)
	text.TextTransparency = 0.5
	text.BackgroundTransparency = 1
	text.TextSize = 24
	text.Size = UDim2.new(1,0,1,0)
	text.Font = Enum.Font.GothamSemibold
	text.Name = "Text"
end

